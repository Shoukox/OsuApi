<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>Shoukko</Authors>
    <Description>osu! api V2 wrapper. (Supported are beatmaps, users, scores apiv2 methods)</Description>
    <PackageProjectUrl></PackageProjectUrl>
    <RepositoryUrl>https://github.com/Shoukox/OsuApi</RepositoryUrl>
    <PackageTags>osu; api</PackageTags>
    <Version>0.0.1</Version>
  </PropertyGroup>

</Project>
